{
    "name": "q-templates-application",
    "version": "10.0.2",
    "description": "Apply for compensation questionnaire template",
    "main": "dist/template.json",
    "engines": {
        "npm": ">=8.5.2",
        "node": ">=16.0.0"
    },
    "scripts": {
        "pretest": "npm run build",
        "test": "jest && npm run gauge:test",
        "coverage": "jest --coverage",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "build": "node ./src/build.js",
        "prepare": "npm run build --silent",
        "dev": "nodemon -L -e .js,.json,.njk,.yml --ignore dist/template.json --exec npm run build",
        "gauge:test": "cross-env template_under_test=$npm_package_name#$npm_package_version gauge run --parallel -n=4 specs"
    },
    "author": "CICA",
    "license": "MIT",
    "devDependencies": {
        "ajv-errors": "^1.0.1",
        "ajv-formats-mobile-uk": "github:CriminalInjuriesCompensationAuthority/ajv-formats-mobile-uk#v1.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jest": "^23.3.0",
        "eslint-plugin-prettier": "^3.1.2",
        "husky": "^4.0.3",
        "jest": "^24.9.0",
        "lint-staged": "^9.5.0",
        "nodemon": "^2.0.19",
        "prettier": "^1.19.1",
        "q-template-validator": "github:CriminalInjuriesCompensationAuthority/q-template-validator#v5.4.0"
    },
    "dependencies": {
        "bdd-test-framework": "file:../bdd-test-framework"
    }
}
